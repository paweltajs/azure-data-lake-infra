{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string"
        },
        "rgName": {
            "type": "string"
        },
        "rootDir": {
            "type": "string"
        }
    },
    "variables": {
        "templateUrl_dls_music_lake": "[concat(parameters('rootDir'),'arm-templates/resources/rg-music-lake/dls_music_lake.json')]"
    },
    "resources": [
        {
            "type": "Microsoft.Resources/resourceGroups",
            "apiVersion": "2021-04-01",
            "name": "[parameters('rgName')]",
            "location": "[parameters('location')]",
            "properties": {}
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-10-01",
            "name": "linkedTemplate",
            "properties": {
                "mode": "Incremental",
                "expressionEvaluationOptions": { "scope": "outer" },
                "templateLink": {
                    "uri": "[variables('templateUrl_dls_music_lake')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": { "value": "[parameters('location')]" },
                    "rgName": { "value": "[parameters('rgName')]" }
                }
            }
        }
    ],
    "outputs": {}
}